{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["index.d.ts"],
  "excludeExternals": true,
  "media": "media",
  "out": "docs",
  "customCss": "theme/style.css",
  "replaceText": {
    "inCodeCommentText": false,
    "inCodeCommentTags": false,
    "inIncludedFiles": true,
    "replacements": [
      {
        "pattern": "/lib/mixin_d3.js",
        "replace": "https://github.com/vegertar/mixin_d3/blob/main/lib/mixin_d3.js"
      },
      {
        "pattern": "/__tests__/lib/",
        "replace": "https://github.com/vegertar/mixin_d3/blob/main/__tests__/lib/"
      },
      {
        "pattern": "\\[\\!\\[GH].*\\(https://vegertar.github.io/mixin_d3/\\)",
        "replace": "[![GH](https://img.shields.io/badge/GitHub-100000?style=for-the-badge&logo=github&logoColor=white)](https://github.com/vegertar/mixin_d3)"
      }
    ]
  }
}
